{
  "name": "threeflightsim-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter ./packages/* run build",
    "test": "pnpm -r --filter ./packages/* run test",
    "dev": "pnpm --filter @threeflightsim/sim-client dev"
  }
}
